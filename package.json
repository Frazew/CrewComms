{
  "name": "crewcomms",
  "version": "0.1",
  "description": "A proximity voice chat Discord implementation for the game Among Us",
  "main": "./dist/main.bundle.js",
  "build": {
    "appId": "fr.frazew.crewcomms",
    "productName": "CrewComms",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "Frazew @frazew",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "pre-pack": "electron-compile --appDir . ./src",
    "package": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "electron-webpack dev"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.42",
    "@types/node": "12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "electron": "9.3.3",
    "electron-builder": "^22.9.1",
    "electron-compilers": "^5.9.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "ts-interface-builder": "^0.2.2",
    "typescript": "^4.0.3",
    "webpack": "~4.42.1"
  },
  "dependencies": {
    "@among-js/data": "../CrewCommsModules/among-js/packages/data/dist",
    "@among-js/sus": "../CrewCommsModules/among-js/packages/sus/dist",
    "@among-js/packets": "../CrewCommsModules/among-js/packages/packets/dist",
    "@among-js/hazel": "../CrewCommsModules/among-js/packages/hazel/dist",
    "@among-js/util": "../CrewCommsModules/among-js/packages/util/dist",
    "@type/discord-rpc": "../CrewCommsModules/@types/discord-rpc",
    "discord-rpc": "../CrewCommsModules/discord-rpc",
    "bytebuffer": "^5.0.1",
    "electron-compile": "^6.4.3",
    "kr-udp-proxy": "^1.0.12",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Frazew/CrewComms"
  },
  "keywords": [
    "discord",
    "among-us",
    "amongus",
    "voice",
    "proximity",
    "electron",
    "react",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/Frazew/CrewComms/issues"
  },
  "homepage": "https://github.com/Frazew/CrewComms#readme"
}
